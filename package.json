{
	"name": "mdexpr-agenda",
	"version": "0.5.0",
	"author": "Jan Andrle <andrle.jan@centrum.cz>",
	"license": "MIT",
	"description": "Use extended markdown syntax to simulate `org-agenda` from emacs.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jaandrle/mdexpr-agenda.git"
	},
	"bugs": {
		"url": "https://github.com/jaandrle/mdexpr-agenda/issues"
	},
	"homepage": "https://github.com/jaandrle/mdexpr-agenda",
